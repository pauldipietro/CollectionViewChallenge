    x:Class="CollectionViewChallenge.Views.CollectionViewChallengePage">
    <ContentPage.Resources>
        <ResourceDictionary>
            <Color x:Key="NavigationPrimary">#2196F3</Color>
            <conv:RupiahFormatConverter x:Key="RupiahFormat" />
            <DataTemplate x:Key="flightTicket">
                <StackLayout Spacing="0">
                    <Grid Margin="0,0,0,10">
                        <BoxView Color="White" Margin="0,0,20,0" CornerRadius="0,20,0,20" />
                        <Grid Padding="10" ColumnSpacing="0" Margin="0,0,20,0" RowSpacing="13">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="2" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <StackLayout Spacing="0">
                                <Label Text="{Binding BoardingTime}" FontSize="23" TextColor="#444" FontAttributes="Bold" HorizontalTextAlignment="Center" />
                                <Label Text="{Binding BoardingAirport}" HorizontalTextAlignment="Center" FontSize="Micro" />
                            </StackLayout>
                            <StackLayout Spacing="0" Grid.Column="2">
                                <Label Text="{Binding DepartureTime}" FontSize="23" TextColor="#444" FontAttributes="Bold" HorizontalTextAlignment="Center" />
                                <Label Text="{Binding DepartureAirport}" HorizontalTextAlignment="Center" FontSize="Micro" />
                            </StackLayout>
                            <Image Source="icon_alert_circle.png" Opacity="0.3" RotationX="180" WidthRequest="25" HeightRequest="25" Grid.Column="3" />
                            <StackLayout Grid.Column="1">
                                <Label Text="{Binding FlightTime}" TextColor="#333" FontSize="10" HorizontalOptions="Center" HorizontalTextAlignment="Center" />
                                <Grid ColumnSpacing="3">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <BoxView HeightRequest="1" Color="#B5B5B5" VerticalOptions="Center" />
                                    <Frame VerticalOptions="Center" BackgroundColor="{StaticResource NavigationPrimary}" HorizontalOptions="Center" WidthRequest="8" HeightRequest="8" CornerRadius="4" HasShadow="false" Grid.Column="1" Padding="0" />
                                    <BoxView HeightRequest="1" Color="#B5B5B5" Grid.Column="2" VerticalOptions="Center" />
                                    <Image Source="icon_plane.png" WidthRequest="15" HeightRequest="15" Grid.Column="3" VerticalOptions="Center" />
                                </Grid>
                                <Label Text="{Binding Stops, StringFormat='{0} stops'}" TextColor="#333" FontSize="10" HorizontalOptions="Center" />
                            </StackLayout>
                            <Image Source="dashed_line_5.png" Aspect="Fill" Grid.Row="1" Grid.ColumnSpan="3" />
                            <Grid Grid.Row="2" Grid.ColumnSpan="4" RowSpacing="0" ColumnSpacing="10">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Image Source="{Binding AirlineLogo}" d:Source="unknown_airline.png" WidthRequest="40" HeightRequest="40" Grid.RowSpan="2" />
                                <StackLayout Grid.Column="1" Margin="0,5,0,0" Spacing="0" VerticalOptions="CenterAndExpand">
                                    <Label TextDecorations="Strikethrough" Text="{Binding PriceCoret, Converter={StaticResource RupiahFormat}}" TextColor="#aaa" FontSize="Small" />
                                    <Label Text="{Binding Price, Converter={StaticResource RupiahFormat}}" TextColor="Red" FontAttributes="Bold" FontSize="Small" />
                                </StackLayout>
                                <Frame HasShadow="false" Padding="5" BackgroundColor="{StaticResource NavigationPrimary}" CornerRadius="20" Grid.RowSpan="2" Grid.Column="3">
                                    <Label Text="PILIH" HorizontalOptions="CenterAndExpand" TextColor="White" VerticalOptions="CenterAndExpand" />
                                </Frame>
                            </Grid>
                        </Grid>
                    </Grid>
                </StackLayout>
            </DataTemplate>
            <DataTemplate x:Key="blankHeader">
                <BoxView HeightRequest="60" />
            </DataTemplate>
            <DataTemplate x:Key="blankFooter">
                <BoxView HeightRequest="50" />
            </DataTemplate>
            <local:ItemSelector x:Key="ItemSelector" FlightTemplate="{StaticResource flightTicket}" BlankFooter="{StaticResource blankFooter}" BlankHeader="{StaticResource blankHeader}" />
        </ResourceDictionary>
